<% unless @membered_artists.empty? %>
    <h2>Your Artists</h2>
    <ul class="list requests small">
      <%- @membered_artists.each do |member| -%>
          <li>
            <!--<div class="artist-image">-->
              <!-- image here -->
            <!--</div>-->
            <p><%= artist_link member.artist %></p>

            <p class="meta published">
              <%= time_ago_in_words(member.created_at) %>
              ago
            </p>
          </li>
      <%- end -%>
    </ul>
<% end %>