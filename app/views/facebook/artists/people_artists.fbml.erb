<%= @person.name %>

<% @person.members.each do |artist_member|
    artist = artist_member.artist
%>
    <div class="jamfu_artist">
      Artist name: <%= artist.name %>

      <%= player_link(artist) %>

    </div>

    <%- artist.albums do |album| -%>
        <%- album.songs do |song| -%>
            <!--<fb:mp3 src="http://<%= JAMFU_HOST %><%= song.public_filename %>" artist="blah" song="blah"/>-->
        <%- end -%>

    <%- end -%>


<%- end -%>


<br/><br/>Owner id : <%= @owner_id %>